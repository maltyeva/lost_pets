    <h5><%= link_to pet.name, pet_path(pet)%></h5>
    <p>Found at <%= pet.address %> on <%= pet.found_on.strftime("%A, %b %d") %>
      <%= link_to "Delete", pet_path(pet), method: :delete, data: {confirm: "Are you sure? ðŸ˜¿"} %></p>
  </div>
<% end %>
